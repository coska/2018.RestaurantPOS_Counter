{
    "env": {
      "node": true,
      "browser": true,
      "es6": true
    },
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
      "react/jsx-filename-extension": [ 1, { "extensions": [ ".js", ".jsx" ] } ],
      "semi": ["error", "never"],
      "linebreak-style": "off",
      "no-use-before-define": "off",
      "react/prop-types": 0
    }
}
